{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "baseUrl": "."
    },
    "include": ["src/**/*"],
    "exclude": ["src/model/*.ts", "node_modules", ".vscode"]
}
